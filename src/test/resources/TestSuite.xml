<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="EPAM_e.popovich" parallel="tests" thread-count="2">
    <parameter name="emailText" value="epam_e.popovich"/>
    <test name="Test @tut.by EMAIL" group-by-instances="true">
        <parameter name="dataBaseLocation" value="accounts.csv"/>
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="demo.test.tests.TutByEmailTest"/>
        </classes>
    </test>
    <test name="Test @tut.by EMAIL2" group-by-instances="true">
        <parameter name="dataBaseLocation" value="accounts.xml"/>
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="demo.test.tests.TutByEmailTest"/>
        </classes>
    </test>
</suite>